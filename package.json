{
  "name": "window-door-mes-ui-vue",
  "version": "0.2.11",
  "description": "A window and door mes softwore",
  "private": true,
  "author": "Yangxiaoyu@example.com",
  "main": "electron/main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "start-mock": "node start-mock-server.js",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve --mode development",
    "start": "concurrently \"vue-cli-service serve --mode development\" \"npm run start-mock\"",
    "electron:build": "electron-builder",
    "electron:dev": "electron .",
    "build:dev": "cross-env NODE_ENV=development vue-cli-service build && electron-builder",
    "build:stg": "cross-env NODE_ENV=staging vue-cli-service build && electron-builder",
    "build:prod": "cross-env NODE_ENV=production vue-cli-service build && electron-builder",
    "build:prod-publish": "cross-env NODE_ENV=production vue-cli-service build && electron-builder  --publish always"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@pattern-x/gemini-viewer-threejs": "^0.2.71",
    "@stomp/stompjs": "^7.0.0",
    "axios": "^1.6.8",
    "core-js": "^3.8.3",
    "dxf-parser": "^1.1.2",
    "echarts": "^5.5.0",
    "electron-log": "^5.1.5",
    "electron-updater": "^6.2.1",
    "element-plus": "^2.7.0",
    "md5": "^2.3.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "rm": "^0.1.8",
    "screenfull": "^6.0.2",
    "sockjs-client": "^1.6.1",
    "sortablejs": "^1.15.2",
    "three": "^0.165.0",
    "three-dxf": "^1.3.1",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-draggable-plus": "^0.5.2",
    "vue-i18n": "^9.12.1",
    "vue-router": "^4.0.3",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.0",
    "@capacitor/core": "^6.1.0",
    "@types/echarts": "^4.9.22",
    "@types/jest": "^27.0.1",
    "@types/md5": "^2.3.5",
    "@types/nprogress": "^0.2.3",
    "@types/sockjs-client": "^1.5.4",
    "@types/sortablejs": "^1.15.8",
    "@types/three": "^0.165.0",
    "@types/vuedraggable": "^2.24.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.3",
    "electron": "^22.0.0",
    "electron-builder": "^22.14.13",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.0.5",
    "mockjs": "^1.1.0",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel"
  },
  "build": {
    "appId": "com.example.windowdoor",
    "productName": "Window Door MES",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "electron/main.js",
      "public/favicon.icon"
    ],
    "win": {
      "icon": "public/favicon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "verifyUpdateCodeSignature": false,
      "forceCodeSigning": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Window Door MES",
      "installerIcon": "public/favicon.ico",
      "uninstallerIcon": "public/favicon.ico",
      "installerHeaderIcon": "public/favicon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "yangxiaoyu2020",
        "repo": "window-door-mes-ui-vue",
        "releaseType": "release"
      }
    ]
  }
}
